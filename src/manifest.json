{
  "name": "screwmycode.in for YouTube",
  "short_name": "screwmycode.in",
  "description": "Variable speed pitch control for YouTube",
  "version": "1.2.8",
  "manifest_version": 2,
  "icons": {
    "16": "assets/icon.jpg",
    "128": "assets/icon.jpg"
  },
  "browser_action": {
    "default_icon": {
      "19": "assets/icon.jpg",
      "38": "assets/icon.jpg"
    },
    "default_title": "screwmycode.in for YouTube",
    "default_popup": "popup/index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/watch*"
      ],
      "js": [
        "scripts/content.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "permissions": [
    "storage",
    "tabs"
  ]
}
